{
  "name": "battle-mappr",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run clean && concurrently --kill-others \"npm run watch:dev\" \"npm run serve:dev\"",
    "build:dev": "npm run svg-sprite && npm run clean && webpack --mode=development --config ./webpack.config.js",
    "watch:dev": "webpack --mode=development --watch --config ./webpack.config.js",
    "serve:dev": "nodemon --watch src/server -r dotenv/config ./src/server/index.js",
    "build": "npm run svg-sprite && npm run clean && webpack --mode=production --config ./webpack.config.js",
    "start": "node ./src/server/index.js",
    "clean": "rimraf .build",
    "svg-sprite": "svg-sprite -s --si --symbol-dest=src/server/views/partials --symbol-sprite=svg-sprite.ejs src/icons/*.svg",
    "lint": "eslint --ext .js ."
  },
  "dependencies": {
    "@teamwork/websocket-json-stream": "^2.0.0",
    "compression": "^1.7.4",
    "cuid": "^2.1.8",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "reconnecting-websocket": "^4.4.0",
    "sharedb": "^1.7.1",
    "sharedb-mongo": "^1.0.0-beta.17",
    "simplify-flat-array": "^1.0.0",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.0.0",
    "css-loader": "^5.1.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "file-loader": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "mini-css-extract-plugin": "^1.3.9",
    "nodemon": "^2.0.7",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "svg-sprite": "^1.5.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.25.0",
    "webpack-cli": "^4.5.0"
  }
}
